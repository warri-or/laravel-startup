{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3",
        "ext-intl": "*",
        "ext-json": "*",
        "afoolzh/iseed": "^1.0",
        "braintree/braintree_php": "^6.2",
        "doctrine/dbal": "^3.1",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.5",
        "joedixon/laravel-translation": "^1.1",
        "jonykpi/laravel-websockets": "^1.0",
        "laravel/framework": "^8.0",
        "laravel/passport": "^10.1",
        "laravel/socialite": "^5.2",
        "laravel/tinker": "^2.0",
        "lcobucci/jwt": "^3.3.3",
        "pusher/pusher-php-server": "^5.0",
        "rap2hpoutre/laravel-log-viewer": "^1.7",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "spatie/laravel-translatable": "^4.6",
        "srmklive/paypal": "~1.0",
        "stripe/stripe-php": "^7.79",
        "yajra/laravel-datatables-oracle": "~9.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "facade/ignition": "^2.3.6",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/Helper/coreArray.php",
            "app/Helper/coreConstants.php",
            "app/Helper/mainHelper.php",
            "app/Helper/permissionHelper.php",
            "app/Helper/filesHelper.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
